_badge_lmi": false, "show_skill_tag_links": true, "translatable": false, "image": {"id": "9e6534f5-4665-43ff-9ab6-3addded81ba7", "url": "https://images.credly.com/images/9e6534f5-4665-43ff-9ab6-3addded81ba7/image.png"}, "image_url": "https://images.credly.com/images/9e6534f5-4665-43ff-9ab6-3addded81ba7/image.png", "url": "https://www.credly.com/org/echelon-front/badge/the-ooda-loop-strategy-and-tactics-course-completio", "issuer": {"summary": "issued by Echelon Front", "entities": [{"label": "Issued by", "primary": true, "entity": {"type": "Organization", "id": "80be93ab-86cf-42e0-9715-26f72e8abf11", "name": "Echelon Front", "url": "https://www.credly.com/api/v1/organizations/80be93ab-86cf-42e0-9715-26f72e8abf11", "vanity_url": "https://www.credly.com/org/echelon-front", "internationalize_badge_templates": false, "share_to_ziprecruiter": true, "verified": false}}]}, "alignments": [], "badge_template_activities": [{"id": "0cd7ef24-a78b-4899-9835-7c874480a49f", "activity_type": "Course", "required_badge_template_id": null, "title": "The OODA Loop is an analytical decision-making tool. It\u2019s an acronym that stands for Observe, Orient, Decide, Act. This four-step process was developed by Colonel John Boyd, a fighter pilot in the 1950s. The OODA Loop is a tool for taking ownership of your environment. As a leader, you need to make decisions, and you need to take action. Learning Objectives: Explain the origin and meaning of the OODA loop, Explain the application of the OODA loop, and Connect the OODA loop to decision-making.", "url": "https://academy.echelonfront.com/"}], "endorsements": [], "skills": [{"id": "ff5881e1-438e-4a51-bb96-48dfa5020789", "name": "Analytical Thinking", "vanity_slug": "analytical-thinking"}, {"id": "d7727e30-86a3-40b0-9c3b-b85f90e1f415", "name": "Decision Making", "vanity_slug": "decision-making"}, {"id": "6eef727c-756c-4b23-8b73-ab8e05b31bd2", "name": "Leadership Development", "vanity_slug": "leadership-development"}]}, "c0afb7f8-bf73-4b87-